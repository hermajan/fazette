{
	"name": "hermajan/fazette",
	"description": "Extensions for Nette framework.",
	"type": "library",
	"homepage": "https://hermajan.net/fazette",
	"support": {
		"issues": "https://github.com/hermajan/fazette/issues"
	},
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"Fazette\\": "src"
		}
	},
	"config": {
		"vendor-dir": "vendor",
		"sort-packages": true,
		"optimize-autoloader": true
	},
	"require": {
		"php": "^7.1"
	},
	"require-dev": {
		"hermajan/booty": "dev-master",
		"latte/latte": "^2.4",
		"nette/forms": "^3.0",
		"nette/tester": "^2.0",
		"nette/utils": "^3.0",
		"tracy/tracy": "^2.5"
	}
}
